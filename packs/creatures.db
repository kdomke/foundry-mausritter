poreal&nbsp;</strong></td>\n<td style=\"width: 68.3544%; height: 16px;\">Float into wall/floor, reappear elsewhere</td>\n</tr>\n</tbody>\n</table>\n<p>&nbsp;</p>","description":{"disposition":"freedom from the pain that binds them to the mortal realm"},"stats":{"strength":{"value":5,"label":"Strength","max":5},"dexterity":{"value":10,"label":"Dexterity","max":10},"will":{"value":10,"label":"Will","max":10}}},"sort":100001,"flags":{},"img":"icons/svg/mystery-man.svg","token":{"flags":{},"name":"Ghost","displayName":20,"img":"icons/svg/mystery-man.svg","tint":null,"width":1,"height":1,"scale":1,"lockRotation":false,"rotation":0,"vision":false,"dimSight":0,"brightSight":0,"dimLight":0,"brightLight":0,"sightAngle":360,"lightAngle":360,"lightAlpha":1,"lightAnimation":{"speed":5,"intensity":5},"actorId":"5o4Cbd94vF3W3xTL","actorLink":false,"disposition":-1,"displayBars":20,"bar1":{"attribute":"health"},"bar2":{},"randomImg":false},"items":[{"_id":"8WQENJawSZFWvV2P","name":"Spectral","entity":"condition","data":{"description":"","sheet":{"active":false,"currentX":1,"currentY":-59,"initialX":1,"initialY":-59,"xOffset":1,"yOffset":-59,"rotation":0,"curWidth":1,"curHeight":1,"zIndex":1},"pips":{"value":0,"max":0,"html":""},"size":{"width":1,"height":1,"x":"9em","y":"9em","aspect":1},"clear":"","desc":"Only damaged by silver or magic weapons"},"sort":100000,"flags":{},"effects":[]},{"_id":"s4jbNbC8xMElrSgb","name":"Chilling Touch","entity":"weapon","data":{"description":"","sheet":{"active":false,"currentX":-129,"currentY":-59,"initialX":-129,"initialY":-59,"xOffset":-129,"yOffset":-59,"rotation":0,"curWidth":1,"curHeight":1,"zIndex":1},"pips":{"value":0,"max":0,"html":""},"size":{"width":1,"height":1,"x":"9em","y":"9em","aspect":1},"weight":0,"cost":0,"tag":"DMG Will","placement":"hand","weapon":{"dmg1":"d8","dmg2":"","selected":0,"canSwap":false}},"sort":200000,"flags":{},"effects":[]},{"_id":"jMmGnBW17BsKwpUz","name":"Ghostly Power","entity":"condition","data":{"description